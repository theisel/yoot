{
  "name": "@yoot/yoot",
  "version": "0.2.1",
  "license": "ISC",
  "exports": {
    ".": "./src/index.ts",
    "./html": "./src/html.ts",
    "./internal": "./src/internal.ts",
    "./jsx": "./src/jsx.ts"
  },
  "compilerOptions": {
    "lib": ["deno.ns", "dom", "esnext"]
  },
  "publish": {
    "include": ["src", "deno.json", "LICENSE", "README.md"],
