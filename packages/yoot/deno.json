    "yoot-adapter",
    "image",
    "images",
    "cdn",
    "responsive",
    "optimization",
    "transform",
    "transformation",
    "url",
    "urlbuilder",
    "img",
    "picture",
    "srcset"
  ],
  "imports": {}
}
